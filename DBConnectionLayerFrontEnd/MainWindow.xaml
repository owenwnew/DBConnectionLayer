<Window x:Class="DBConnectionLayerFrontEnd.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DBConnectionLayerFrontEnd"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <StackPanel Orientation="Vertical">
            <Button Height="50" Content="Connection Test" Command="{Binding ConnectToDB}"></Button>
            <Button Height="50" Content="Insert to DB" Command="{Binding InsertToDB}"></Button>
            <Button Height="50" Content="Export From DB"></Button>
        </StackPanel>
        <StackPanel>
            
        </StackPanel>
        
    </Grid>
</Window>
